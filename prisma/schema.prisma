generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model GameSession {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
}

model Scores {
  id             Int    @id @default(autoincrement())
  username       String
  completionTime Int
}
